<?xml version="1.0"?>
<block>
  <name>STS Blocker</name>
  <key>dpd_sts_blocker</key>
  <category>[dpd]</category>
  <import>import dpd</import>
  <make>dpd.sts_blocker($cp_len)</make>
  
  <param>
    <name>Cyclic-Prefix Length</name>
    <key>cp_len</key>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
