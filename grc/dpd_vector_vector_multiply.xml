<?xml version="1.0"?>
<block>
  <name>Vector-Vector Multiply</name>
  <key>dpd_vector_vector_multiply</key>
  <category>[dpd]</category>
  <import>import dpd</import>
  <make>dpd.vector_vector_multiply($const_vec, $vec_len)</make>
 
  <param>
    <name>Constant Vector</name>
    <key>const_vec</key>
    <value>(0,0,0,0,0)</value>
    <type>raw</type>
  </param>
  <param>
    <name>Vector Length</name>
    <key>vec_len</key>
    <type>int</type>
  </param>
  
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>$vec_len</vlen>
  </sink>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
