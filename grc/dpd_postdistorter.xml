<?xml version="1.0"?>
<block>
  <name>Postdistorter</name>
  <key>dpd_postdistorter</key>
  <category>[dpd]</category>
  <import>import dpd</import>
  <make>dpd.postdistorter($dpd_params)</make>

  <param>
    <name>DPD Parameters</name>
    <key>dpd_params</key>
    <value>(0,0,0,0,0)</value>
    <type>raw</type>
  </param>

  <sink>
    <name>PA_input</name>
    <type>message</type>
  </sink>

  <sink>
    <name>in</name>
    <type>complex</type>    
  </sink>

  <source>
	<name>taps</name>
	<type>message</type>
  </source>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
